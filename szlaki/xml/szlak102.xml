    </punkt>
  </odcinek>
  <odcinek id="432" typ="rzeka" nazwa="Odcinek środkowy" 
      uciazliwosc="U2" trudnosc="ZWB" 
      malowniczosc="***" czystosc="KL1" 
      kolejnosc="1"> 
