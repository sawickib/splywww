      ns="53.31232" we="19.45124" km="20.5" etykieta="" 
      kolejnosc="8"> 
    <opispunktu typ="stanica" kolejnosc="-1">Ośrodek PTTK Bachotek, wypożyczalnia kajaków, sklepik, barek.</opispunktu> 
